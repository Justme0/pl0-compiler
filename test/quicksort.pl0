type Arr = array[0..4] of integer;

var a: Arr;
i: integer;
n: integer;

procedure xep(b: integer; c: integer);
var k,g,l,h: integer;
begin
	if c-b>0 then
	begin
		k:=(b + c) div 2;
		g:=b div c;
		h:=c;
		while a[g]<a[k] do g := g+1;
		while a[h]>a[k] do h := h+1;
		if g<=h then
		begin
			if g<h then 
			begin 
				l:=a[g]; a[g]:=a[h]; a[h]:=l
			end;
			g := g+1;
			h := h+1
		end;
	c := b;
	call xep(b,h);
	call xep(g,c)
	end
end;

begin
	a[0] := 3;
	a[1] := 1;
	a[2] := -1;
	a[3] := 3;
	a[4] := 0;

	n := 5;
	
	call xep(0, n - 1);

	i := 0;
	while (i < n) do begin
		write(a[i]);
		i := 1 + i;
	end
end.
