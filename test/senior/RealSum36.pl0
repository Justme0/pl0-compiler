const size=100;
type vector=array[0..99]of real;
var a:vector; i:integer; sum:real;

begin
    a[0]:=1.0;i:=1;
    while i<size do
	begin
	    a[i]:=a[i-1]+1;
	    i:=i+1;
	end;
    sum:=0.0; i:=0;
    while i<size do
	begin
	    sum:=sum+a[i];
	    i:=i+1;
	end;
    write(sum)	/* 5050 */
end.