var n:integer;
procedure movement(u:integer; v:integer);

    begin
	write(u,v)
    end;

procedure move(n:integer; x:integer; y:integer; z:integer);

    begin
        if n>0 then
	    begin
		call move(n-1,x,z,y);
		call movement(x,y);
		call move(n-1,z,y,x)        
            end
    end;

begin
    call move(3,1,2,3)
end.

